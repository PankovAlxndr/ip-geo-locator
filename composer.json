{
  "name": "pankovalxndr/ip-geo-locator",
  "description": "Study project for understanding the Decorator pattern.",
  "type": "library",
  "keywords": [
    "patterns",
    "decorator",
    "wrapper",
    "proxy"
  ],
  "homepage": "https://github.com/pankovalxndr/ip-geo-locator",
  "require": {
    "php": "^8.2",
    "psr/simple-cache": "^3.0",
    "psr/log": "^3.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "vimeo/psalm": "^5.4",
    "squizlabs/php_codesniffer": "^3.7"
  },
  "autoload": {
    "psr-4": {
      "IpGeoLocator\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "IpGeoLocator\\Test\\": "tests/"
    }
  },
  "authors": [
    {
      "name": "pankovalxndr",
      "email": "pankovalxndr@gmail.com"
    }
  ],
  "scripts": {
    "test": "phpunit --colors=always",
    "test-coverage": "XDEBUG_MODE=coverage phpunit --colors=always --coverage-html var/coverage",
    "phpcs": "phpcs --standard=phpcs.xml",
    "phpcbf": "phpcbf",
    "psalm": "psalm --config=psalm.xml"
  }
}
